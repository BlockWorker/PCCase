<UserControl x:Class="CaseControl2.TripleRangeSlider"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:CaseControl2"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800" Height="40" MouseLeftButtonDown="OnMouseDown" MouseLeftButtonUp="OnMouseUp" MouseMove="OnMouseMove">
    <UserControl.Resources>
        <Color x:Key="BackColor">#FF101010</Color>
        <Color x:Key="BorderColor">#FFA0A0A0</Color>
        <Color x:Key="LabelColor">#FF707070</Color>
        <Color x:Key="HandleColor">#FF00A0FF</Color>
        <Color x:Key="GoodColor">#FF00FF70</Color>
        <Color x:Key="WarnColor">#FFFFFF00</Color>
        <Color x:Key="SoftOffColor">#FFFF6000</Color>
        <Color x:Key="HardOffColor">#FFFF0000</Color>
        <Color x:Key="HintColor">#600090ff</Color>
    </UserControl.Resources>
</UserControl>
